<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="hexDB_DLL" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="r_MSW">
				<Option output="bin/app/hexDB" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(TARGET_OUTPUT_DIR)" />
				<Option object_output="obj/app/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add directory="$(#wx)/lib/gcc_dll/mswu" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx)/lib/gcc_dll/mswu" />
				</ResourceCompiler>
				<Linker>
					<Add option="-s" />
					<Add library="user32" />
					<Add library="libwxmsw28u.a" />
					<Add directory="$(#wx)/lib/gcc_dll" />
				</Linker>
			</Target>
			<Target title="d_MSW">
				<Option output="bin/dbg/hexDB" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/dbg/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-D__WXDEBUG__" />
					<Add directory="$(#wx)/lib/gcc_dll/mswud" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx)/lib/gcc_dll/mswud" />
				</ResourceCompiler>
				<Linker>
					<Add library="user32" />
					<Add library="libwxmsw28ud.a" />
					<Add directory="$(#wx)/lib/gcc_dll" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option='[[if (PLATFORM == PLATFORM_MSW &amp;&amp; (GetCompilerFactory().GetCompilerVersionString(_T(&quot;gcc&quot;)) &gt;= _T(&quot;4.0.0&quot;))) print(_T(&quot;-Wno-attributes&quot;));]]' />
			<Add option="-Winvalid-pch" />
			<Add option="-include wx_pch.h" />
			<Add option="-DxsDLL_BUILD" />
			<Add option="-D__GNUWIN32__" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DWX_PRECOMP" />
			<Add directory="$(#wx)/include" />
			<Add directory="$(#wx)/contrib/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#wx)/include" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
		</Linker>
		<Unit filename="hexDB_code.cpp" />
		<Unit filename="hexDB_hack.cpp" />
		<Unit filename="hexDB_hack.hpp" />
		<Unit filename="hexDB_hexIO.cpp" />
		<Unit filename="hexDB_hexIO.hpp" />
		<Unit filename="hexDB_loadHack_hex.cpp" />
		<Unit filename="hexDB_main.cpp" />
		<Unit filename="hexDB_main.hpp" />
		<Unit filename="hexDB_makeHack_hex.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
